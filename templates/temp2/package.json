{
  "name": "projectName",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build && babel src -d build --copy-files && del build/**/__tests__ build/**/__mocks__ build/**/__fixtures__",
    "build:windows": "rimraf -rf build && babel src --out-dir build --ignore src/**/__tests__,src/**/__mocks__,src/**/__fixtures__",
    "docker:build": "docker build -t {project-name} .",
    "docker:run": "docker run -p 8080:8080 -d {project-name} ",
    "dev": "nodemon --watch src --exec babel-node src",
    "decrypt:dev": "gcloud kms decrypt --plaintext-file=.env --ciphertext-file=.env.dev --location=global --keyring={keyRing-name} --key={key-name} --project={project-name}",
    "decrypt:test": "gcloud kms decrypt --plaintext-file=.env --ciphertext-file=.env.test --location=global --keyring={keyRing-name} --key={key-name} --project={project-name}",
    "decrypt:prod": "gcloud kms decrypt --plaintext-file=.env --ciphertext-file=.env.prod --location=global --keyring={keyRing-name} --key={key-name} --project={project-name}",
    "start": "node build/index.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
